<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:ns2="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://fastconnect.fr/fcunit-abstract" xmlns:ns3="http://fastconnect.fr/fcunit.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions">
    <pd:name>FCUnit/runAllTestsAndShowResults.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startX>58</pd:startX>
    <pd:startY>66</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>922</pd:endX>
    <pd:endY>162</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1310545520296</pd:targetNamespace>
    <pd:activity name="runAllTests">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>165</pd:x>
        <pd:y>66</pd:y>
        <config>
            <processName>/FCUnit/Core/Run/runAllTests.process</processName>
        </config>
        <pd:inputBindings>
            <ns:settings-msg>
                <settings>
                    <ns3:settings>
                        <ns3:export-to-files>
                            <xsl:value-of select="&quot;true&quot;"/>
                        </ns3:export-to-files>
                        <ns3:export-directory>
                            <xsl:value-of select="&quot;?&quot;"/>
                        </ns3:export-directory>
                    </ns3:settings>
                </settings>
            </ns:settings-msg>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Output to Console">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>567</pd:x>
        <pd:y>66</pd:y>
        <config>
            <role>User</role>
        </config>
        <pd:inputBindings>
            <ns2:ActivityInput>
                <message>
                    <xsl:value-of select="concat(&quot;&#xA;&quot;,tib:render-xml($runAllTests/ns:test-suites-results-msg/test-suites-results, true(), true()))"/>
                </message>
            </ns2:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Nombre de tests en échec">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>316</pd:x>
        <pd:y>298</pd:y>
        <config>
            <element>
                <xsd:element name="failedTestsCount" type="xsd:int"/>
            </element>
        </config>
        <pd:inputBindings>
            <failedTestsCount>
                <xsl:value-of select="count($runAllTests/ns:test-suites-results-msg/test-suites-results/ns3:test-suites-results//ns3:test-failure)"/>
            </failedTestsCount>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Nombre de processes de test">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>312</pd:x>
        <pd:y>170</pd:y>
        <config>
            <element>
                <xsd:element name="testProcessesNumber" type="xsd:string"/>
            </element>
        </config>
        <pd:inputBindings>
            <testProcessesNumber>
                <xsl:value-of select="$runAllTests/ns:test-suites-results-msg/test-suites-results/ns3:test-suites-results/ns3:TestProcessesNumber"/>
            </testProcessesNumber>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Nombre de processes testables">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>452</pd:x>
        <pd:y>177</pd:y>
        <config>
            <element>
                <xsd:element name="testableProcessesNumber" type="xsd:int"/>
            </element>
        </config>
        <pd:inputBindings>
            <testableProcessesNumber>
                <xsl:value-of select="$runAllTests/ns:test-suites-results-msg/test-suites-results/ns3:test-suites-results/ns3:TestableProcessesNumber"/>
            </testableProcessesNumber>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Nombre de processes testés">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>567</pd:x>
        <pd:y>170</pd:y>
        <config>
            <element>
                <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="testedProcessesNumber" type="xsd:int"/>
            </element>
        </config>
        <pd:inputBindings>
            <testedProcessesNumber>
                <xsl:value-of xmlns:xsl="http://www.w3.org/1999/XSL/Transform" select="$runAllTests/ns:test-suites-results-msg/test-suites-results/ns3:test-suites-results/ns3:TestedProcessesNumber"/>
            </testedProcessesNumber>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Processes de test en échec">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>569</pd:x>
        <pd:y>298</pd:y>
        <config>
            <element>
                <xsd:element name="failed-tests">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="test-process-path" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <failed-tests>
                <xsl:for-each select="$runAllTests/ns:test-suites-results-msg/test-suites-results/ns3:test-suites-results//ns3:test-result[./ns3:test-failure]/@path">
                    <test-process-path>
                        <xsl:value-of select="."/>
                    </test-process-path>
                </xsl:for-each>
            </failed-tests>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Pourcentage de couverture de tests">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>690</pd:x>
        <pd:y>177</pd:y>
        <config>
            <element>
                <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="testCoveragePercentage" type="xsd:double"/>
            </element>
        </config>
        <pd:inputBindings>
            <testCoveragePercentage>
                <xsl:value-of xmlns:xsl="http://www.w3.org/1999/XSL/Transform" select="$runAllTests/ns:test-suites-results-msg/test-suites-results/ns3:test-suites-results/ns3:TestCoveragePercentage"/>
            </testCoveragePercentage>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>runAllTests</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>runAllTests</pd:from>
        <pd:to>Output to Console</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Output to Console</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>runAllTests</pd:from>
        <pd:to>Nombre de tests en échec</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>runAllTests</pd:from>
        <pd:to>Nombre de processes de test</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
