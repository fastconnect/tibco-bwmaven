        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="TestWithXMLInput">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>569</pd:x>
        <pd:y>98</pd:y>
        <config>
            <processName>/FCUnit/Templates/TestWithXMLInput.process</processName>
            <processNameXPath>$Start/pfx:test/@path</processNameXPath>
        </config>
        <pd:inputBindings>
            <test>
                <pfx:test-xml>
                    <xsl:if test="$Start/pfx:test/@name">
                        <xsl:attribute name="name">
                            <xsl:value-of select="$Start/pfx:test/@name"/>
                        </xsl:attribute>
                    </xsl:if>
                    <xsl:if test="$Start/pfx:test/@path">
                        <xsl:attribute name="path">
                            <xsl:value-of select="$Start/pfx:test/@path"/>
                        </xsl:attribute>
                    </xsl:if>
                    <xsl:attribute name="expected-xml-filename">
                        <xsl:value-of select="concat($Get-Project-Base-Dir/ns1:JavaMethodActivityOutput/MethodReturnValue,tib:substring-before-last($Start/pfx:test/@path, &quot;/&quot;), &quot;/expected-data.xml&quot;)"/>
                    </xsl:attribute>
                    <xsl:attribute name="ignored-xpaths-filename">
                        <xsl:value-of select="concat($Get-Project-Base-Dir/ns1:JavaMethodActivityOutput/MethodReturnValue,tib:substring-before-last($Start/pfx:test/@path, &quot;/&quot;), &quot;/ignored-xpaths.xml&quot;)"/>
                    </xsl:attribute>
                    <xsl:attribute name="accept-more-fields-in-actual">
                        <xsl:value-of select="&quot;true&quot;"/>
                    </xsl:attribute>
                </pfx:test-xml>
                <input-data>
                    <xsl:copy-of select="$Parse-XML/*"/>
                </input-data>
            </test>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Get Project Base Dir">
        <pd:type>com.tibco.plugin.java.JavaMethodActivity</pd:type>
        <pd:resourceType>ae.activities.JavaMethodActivity</pd:resourceType>
        <pd:x>244</pd:x>
        <pd:y>106</pd:y>
        <config>
            <ConstructDeclaredClass>true</ConstructDeclaredClass>
            <CacheConstructedClass>true</CacheConstructedClass>
            <InvokeCleanupMethod>false</InvokeCleanupMethod>
            <MethodInfo>
                <classLocation>c:/documents and settings/debovema/.m2/repository/fr/fastconnect/tibco/businessworks/fcunit/fcunit-core/1.3.0-snapshot/fcunit-core-1.3.0-snapshot.jar</classLocation>
                <classPath>/FCUnit/Resources/FCUnit Aliases.aliaslib</classPath>
                <className>fr.fastconnect.tibco.businessworks.fcunit.ProjectBaseDir</className>
                <methodName>getProjectBaseDir</methodName>
                <methodReturn>java.lang.String</methodReturn>
            </MethodInfo>
        </config>
        <pd:inputBindings>
            <ns4:JavaMethodActivityInput/>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Parse XML">
        <pd:type>com.tibco.plugin.xml.XMLParseActivity</pd:type>
        <pd:resourceType>ae.activities.XMLParseActivity</pd:resourceType>
        <pd:x>452</pd:x>
        <pd:y>98</pd:y>
        <config>
            <inputStyle>text</inputStyle>
            <xsdVersion>default</xsdVersion>
            <validateOutput>true</validateOutput>
            <term>
                <xsd:any processContents="skip"/>
            </term>
        </config>
        <pd:inputBindings>
            <xmlString>
                <xsl:value-of select="$Read-File/ns5:ReadActivityOutputTextClass/fileContent/textContent"/>
            </xmlString>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Read File">
        <pd:type>com.tibco.plugin.file.FileReadActivity</pd:type>
        <pd:resourceType>ae.activities.FileReadActivity</pd:resourceType>
        <pd:x>350</pd:x>
        <pd:y>98</pd:y>
        <config>
            <encoding>text</encoding>
        </config>
        <pd:inputBindings>
            <ns12:ReadActivityInputClass>
                <fileName>
                    <xsl:value-of select="concat($Get-Project-Base-Dir/ns1:JavaMethodActivityOutput/MethodReturnValue,tib:substring-before-last($Start/pfx:test/@path, &quot;/&quot;), &quot;/input-data.xml&quot;)"/>
                </fileName>
            </ns12:ReadActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
