<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:bwu="java://org.windyroad.bwunit.CustomFunctions" xmlns:ns5="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:ns12="http://www.tibco.com/namespaces/tnt/plugins/file/v2" xmlns:pfx="http://fastconnect.fr/fcunit.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="com/tibco/pe/commands" xmlns:ns1="www.tibco.com/plugin/java/JavaMethodActivityOutput" xmlns:ns4="www.tibco.com/plugin/java/JavaMethodActivityInput" xmlns:ns3="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx2="http://windyroad.org/bwunit-0.0.0.xsd">
    <xsd:import namespace="http://fastconnect.fr/fcunit.xsd"/>
    <xsd:import namespace="http://fastconnect.fr/fcunit.xsd" schemaLocation="/FCUnit/Schemas/FCUnitSchema.xsd"/>
    <pd:name>FCUnit/Core/Run/runSimpleTestXML.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:test"/>
    <pd:startX>58</pd:startX>
    <pd:startY>98</pd:startY>
    <pd:returnBindings>
        <pfx:test-result>
            <xsl:if test="$Start/pfx:test/@name">
                <xsl:attribute name="name">
                    <xsl:value-of select="$Start/pfx:test/@name"/>
                </xsl:attribute>
            </xsl:if>
            <xsl:if test="$Start/pfx:test/@path">
                <xsl:attribute name="path">
                    <xsl:value-of select="$Start/pfx:test/@path"/>
                </xsl:attribute>
            </xsl:if>
            <xsl:attribute name="datetime">
                <xsl:value-of select="current-dateTime()"/>
            </xsl:attribute>
            <xsl:choose>
                <xsl:when test="$_error">
                    <pfx:test-failure>
                        <pfx:error-report>
                            <pfx:StackTrace>
                                <xsl:value-of select="$_error/ns:ErrorReport/StackTrace"/>
                            </pfx:StackTrace>
                            <pfx:Msg>
                                <xsl:value-of select="$_error/ns:ErrorReport/Msg"/>
                            </pfx:Msg>
                            <pfx:FullClass>
                                <xsl:value-of select="$_error/ns:ErrorReport/FullClass"/>
                            </pfx:FullClass>
                            <pfx:Class>
                                <xsl:value-of select="$_error/ns:ErrorReport/Class"/>
                            </pfx:Class>
                            <pfx:ProcessStack>
                                <xsl:if test="$_error/ns:ErrorReport/ProcessStack">
                                    <pfx:ProcessName>
                                        <xsl:value-of select="$_error/ns:ErrorReport/ProcessStack"/>
                                    </pfx:ProcessName>
                                </xsl:if>
                            </pfx:ProcessStack>
                            <xsl:if test="$_error/ns:ErrorReport/MsgCode">
                                <pfx:MsgCode>
                                    <xsl:value-of select="$_error/ns:ErrorReport/MsgCode"/>
                                </pfx:MsgCode>
                            </xsl:if>
                            <pfx:Data>
                                <xsl:copy-of select="$_error/ns:ErrorReport/Data/ancestor-or-self::*/namespace::node()"/>
                                <xsl:copy-of select="$_error/ns:ErrorReport/Data/@*"/>
                                <xsl:copy-of select="$_error/ns:ErrorReport/Data/node()"/>
                            </pfx:Data>
                        </pfx:error-report>
                    </pfx:test-failure>
                </xsl:when>
                <xsl:otherwise>
                    <pfx:test-success>
                        <pfx:duration>
                            <pfx:test>
                                <xsl:value-of select="$Get-Completion-Time/ns2:output/ns2:GetProcessInstanceInfo[1]/ElapsedTimeSinceStarted - $Get-Start-Time/ns2:output/ns2:GetProcessInstanceInfo[1]/ElapsedTimeSinceStarted"/>
                            </pfx:test>
                        </pfx:duration>
                    </pfx:test-success>
                </xsl:otherwise>
            </xsl:choose>
        </pfx:test-result>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx:test-result"/>
    <pd:endX>890</pd:endX>
    <pd:endY>98</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
